package exercise3;
public class User {
    private final String name, username;
    private final int password;
    private final String song;
    private final boolean premiumAccount;
    
    public User(String n, String user, int pass, String s, boolean p) {
        this.name = n;
        this.username = user;
        this.password = pass;
        this.song = s;
        this.premiumAccount = p;
        
    }
    
    public String getName(){
        return name;
    }
    public String getUsername(){
        return username;
    }
    public int getPassword(){
        return password;
    }
    public String getSong(){
        return song;
    }
    public boolean getPremiumStatus(){
        return premiumAccount;
    }
}
